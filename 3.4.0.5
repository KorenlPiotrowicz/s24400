#include <iostream>
#include <array>
#include <vector>

const unsigned INDEX_NUMBER = 24400;

std::vector<int> copyTableToVector(std::array<std::array<int, 8>, 100> arr) {
    unsigned short tmp = INDEX_NUMBER%3;
    if(tmp == 0)
        tmp = 5;

    std::vector<int> a;
    for(int i = 0; i < 8; ++i) {
        a.push_back(arr[tmp][i]);
    }
    return a;
}

int main() {
    std::array<std::array<int, 8>, 100> arr;
    for(int i = 0; i < 100; ++i) {
        for(int j = 0; j < 8; ++j) {
            arr[i][j] = i*j;
        }
    }
    std::vector<int> a = copyTableToVector(arr);

    for(auto const &i : a) 
    {
        std::cout << i << " ";
    }
    std::cout << "\n";

    return 0;
}
